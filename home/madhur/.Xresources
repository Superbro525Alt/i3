

Xft.dpi: 120
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xcursor.size: 48
Xcursor.theme: Simp1e-Tokyo-Night

! X colors.
! Generated by 'wal'
*foreground:        #a2a8ba
*background:        #1f252d
*.foreground:       #a2a8ba
*.background:       #1f252d
emacs*foreground:   #a2a8ba
emacs*background:   #1f252d
URxvt*foreground:   #a2a8ba
XTerm*foreground:   #a2a8ba
UXTerm*foreground:  #a2a8ba
URxvt*background:   [100]#1f252d
XTerm*background:   #1f252d
UXTerm*background:  #1f252d
URxvt*cursorColor:  #a2a8ba
XTerm*cursorColor:  #a2a8ba
UXTerm*cursorColor: #a2a8ba
URxvt*borderColor:  [100]#1f252d

! Colors 0-15.
*.color0: #1f252d
*color0:  #1f252d
*.color1: #cb8d56
*color1:  #cb8d56
*.color2: #8ba089
*color2:  #8ba089
*.color3: #a79c46
*color3:  #a79c46
*.color4: #8c9abe
*color4:  #8c9abe
*.color5: #b190af
*color5:  #b190af
*.color6: #8e9aba
*color6:  #8e9aba
*.color7: #a2a8ba
*color7:  #a2a8ba
*.color8: #a29899
*color8:  #a29899
*.color9: #d39d74
*color9:  #d39d74
*.color10: #80b48f
*color10:  #80b48f
*.color11: #bda75a
*color11:  #bda75a
*.color12: #9ca5de
*color12:  #9ca5de
*.color13: #c69ac6
*color13:  #c69ac6
*.color14: #8caeb6
*color14:  #8caeb6
*.color15: #a2a8ba
*color15:  #a2a8ba

! Black color that will not be affected by bold highlighting.
*.color66: #1f252d
*color66:  #1f252d

! Xclock colors.
XClock*foreground: #a2a8ba
XClock*background: #1f252d
XClock*majorColor:  rgba:a2/a8/ba/ff
XClock*minorColor:  rgba:a2/a8/ba/ff
XClock*hourColor:   rgba:a2/a8/ba/ff
XClock*minuteColor: rgba:a2/a8/ba/ff
XClock*secondColor: rgba:a2/a8/ba/ff

! Set depth to make transparency work.
URxvt*depth: 32
